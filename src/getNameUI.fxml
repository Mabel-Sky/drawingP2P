<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="142.0" prefWidth="300.0" spacing="10" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <Text fx:id="textField" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
    <TextField fx:id="nameField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Your Name Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
    <Button fx:id="goButton" text="GO!">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
</VBox>
